<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite thread-count="1" verbose="1" name="WebFormsCore">
	<test name="WebformsPersistanceTest" enabled="true"
		preserve-order="true">
		<groups>
			<run>
				<include name="formElements" />
 				<include name="blockTest" />
 				<include name="ruleTest" />
 				<include name="removeElement" />
				<include name="blockLink" />
				<include name="persistForm" />
				<include name="formReference" />
				<include name="webserviceDefinitionJsonTest" />
				<include name="jsonTests"/>
			</run>
		</groups>
		<classes>
			<class name="com.biit.webforms.persistence.EhCacheTest" />
			<class name="com.biit.webforms.persistence.FormElements" />
			<class name="com.biit.webforms.persistence.BlockTests" />
			<class name="com.biit.webforms.persistence.RuleTest" />
			<class name="com.biit.webforms.persistence.RemoveTest" />
			<class name="com.biit.webforms.persistence.BlockLinkTests" />
			<class name="com.biit.webforms.persistence.PersistForm" />
			<class name="com.biit.webforms.persistence.FormReference" />
			<class name="com.biit.webforms.persistence.WebserviceDefinitionJsonTest" />
			<class name="com.biit.webforms.persistence.FormToJsonTests" />
			<class name="com.biit.webforms.persistence.JsonToFormTests" />
		</classes>
	</test>
</suite>

