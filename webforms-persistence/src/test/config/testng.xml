<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite thread-count="1" verbose="1" name="WebFormsCore">
	<test name="WebformsPersistanceTest" enabled="true" preserve-order="true">
		<groups>
			<run>
				<exclude name="ehCache" />
				<include name="blockTest" />
				<include name="ruleTest" />
				<include name="formElements" />
				<include name="blockLink" />
			</run>
		</groups>
		<classes>
			<class name="com.biit.webforms.persistence.EhCacheTest" />
			<class name="com.biit.webforms.persistence.RuleTest" />
			<class name="com.biit.webforms.persistence.FormElements" />
			<class name="com.biit.webforms.persistence.BlockTests" />
			<class name="com.biit.webforms.persistence.BlockLinkTests" />
		</classes>
	</test>
</suite>

