{
  "class": "com.biit.webforms.webservices.Webservice",
  "name": "getUserData",
  "description": "Gets currently logged in user data",
  "protocol": "http",
  "host": "localhost",
  "port": "8081",
  "path": "rest/getUser",
  "inputPorts": [
    {
      "errorCodes": [
        "not exist",
        "not valid"
      ],
      "class": "com.biit.webforms.webservices.WebserviceValidatedPort",
      "name": "bsn",
      "xpath": "/bsn",
      "validationXpath": "/valid",
      "type": "INPUT",
      "format": "TEXT",
      "subformat": "BSN"
    }
  ],
  "outputPorts": [
    {
      "class": "com.biit.webforms.webservices.WebservicePort",
      "name": "name",
      "xpath": "/PersonalInfo/name",
      "type": "INPUT",
      "format": "TEXT",
      "subformat": "TEXT"
    },
    {
      "class": "com.biit.webforms.webservices.WebservicePort",
      "name": "surname",
      "xpath": "/PersonalInfo/surname",
      "type": "INPUT",
      "format": "TEXT",
      "subformat": "TEXT"
    },
     {
       "class": "com.biit.webforms.webservices.WebservicePort",
      "name": "street",
      "xpath": "/PersonalInfo/Street",
      "type": "INPUT",
      "format": "TEXT",
      "subformat": "TEXT"
    },
    {
      "class": "com.biit.webforms.webservices.WebservicePort",
      "name": "streetNumber",
      "xpath": "/PersonalInfo/StreetNumber",
      "type": "INPUT",
      "format": "NUMBER",
      "subformat": "NUMBER"
    },
    {
      "class": "com.biit.webforms.webservices.WebservicePort",
      "name": "postCode",
      "xpath": "/PersonalInfo/PostalCode",
      "type": "INPUT",
      "format": "POSTAL_CODE",
      "subformat": "POSTAL_CODE"
    }
  ]
}