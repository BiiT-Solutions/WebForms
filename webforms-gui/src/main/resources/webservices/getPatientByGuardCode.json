{
  "class": "com.biit.webforms.webservices.Webservice",
  "name": "getPatientByGuardCode",
  "description": "Gets customer data from an autogenerated guard",
  "protocol": "http",
  "host": "localhost",
  "port": "8081",
  "path": "rest/getPatientByGuardCode",
  "inputPorts": [
    {
      "errorCodes": [
        "not exist",
        "not valid"
      ],
      "class": "com.biit.webforms.webservices.WebserviceValidatedPort",
      "name": "user_guard",
      "xpath": "/userguard",
      "validationXpath": "/valid",
      "type": "INPUT",
      "format": "TEXT",
      "subformat": "TEXT"
    }
  ],
  "outputPorts": [
    {
      "class": "com.biit.webforms.webservices.WebservicePort",
      "name": "firstname",
      "xpath": "/PersonalInfo/name",
      "type": "INPUT",
      "format": "TEXT",
      "subformat": "TEXT"
    },
    {
      "class": "com.biit.webforms.webservices.WebservicePort",
      "name": "lastname",
      "xpath": "/PersonalInfo/surname",
      "type": "INPUT",
      "format": "TEXT",
      "subformat": "TEXT"
    },
    {
      "class": "com.biit.webforms.webservices.WebservicePort",
      "name": "initials",
      "xpath": "/PersonalInfo/initials",
      "type": "INPUT",
      "format": "TEXT",
      "subformat": "TEXT"
    },
    {
      "class": "com.biit.webforms.webservices.WebservicePort",
      "name": "gender",
      "xpath": "/PersonalInfo/gender",
      "type": "SINGLE_SELECTION_RADIO"
    },
    {
      "class": "com.biit.webforms.webservices.WebservicePort",
      "name": "birthdate",
      "xpath": "/PersonalInfo/birthday",
      "type": "INPUT",
      "format": "DATE",
      "subformat": "DATE_PAST"
    },
    {
      "class": "com.biit.webforms.webservices.WebservicePort",
      "name": "address",
      "xpath": "/PersonalInfo/street",
      "type": "INPUT",
      "format": "TEXT",
      "subformat": "TEXT"
    },
    {
      "class": "com.biit.webforms.webservices.WebservicePort",
      "name": "postalcode",
      "xpath": "/PersonalInfo/PostalCode",
      "type": "INPUT",
      "format": "POSTAL_CODE",
      "subformat": "POSTAL_CODE"
    },
    {
      "class": "com.biit.webforms.webservices.WebservicePort",
      "name": "city",
      "xpath": "/PersonalInfo/city",
      "type": "INPUT",
      "format": "TEXT",
      "subformat": "TEXT"
    },
    {
      "class": "com.biit.webforms.webservices.WebservicePort",
      "name": "mobilePhone",
      "xpath": "/PersonalInfo/phone",
      "type": "INPUT",
      "format": "TEXT",
      "subformat": "PHONE"
    },
    {
      "class": "com.biit.webforms.webservices.WebservicePort",
      "name": "email",
      "xpath": "/PersonalInfo/email",
      "type": "INPUT",
      "format": "TEXT",
      "subformat": "EMAIL"
    },
    {
      "class": "com.biit.webforms.webservices.WebservicePort",
      "name": "comparationId",
      "xpath": "/PersonalInfo/comparationId",
      "type": "SYSTEM_FIELD",
      "format": "TEXT",
      "subformat": "TEXT"
    }
  ]
}