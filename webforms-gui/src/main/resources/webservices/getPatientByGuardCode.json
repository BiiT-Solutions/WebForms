{
	"name": "getPatientByGuardCode",
	"description": "Gets customer data from an autogenerated guard",
	"protocol": "http",
	"host": "localhost",
	"port": "8081",
	"path": "rest/getPatientByGuardCode",
	"inputPorts": [
		{
			"errorCodes": [
				"not exist",
				"not valid"
			],
			"name": "user_guard",
			"xpath": "/userguard",
			"validationXpath": "/valid",
			"type": "INPUT",
			"format": "TEXT",
			"subformat": "TEXT"
		}
	],
	"outputPorts": [
		{
			"name": "firstname",
			"xpath": "/PersonalInfo/name",
			"type": "INPUT",
			"format": "TEXT",
			"subformat": "TEXT"
		},
		{
			"name": "lastname",
			"xpath": "/PersonalInfo/surname",
			"type": "INPUT",
			"format": "TEXT",
			"subformat": "TEXT"
		},
		{
			"name": "initials",
			"xpath": "/PersonalInfo/initials",
			"type": "INPUT",
			"format": "TEXT",
			"subformat": "TEXT"
		},
		{
			"name": "gender",
			"xpath": "/PersonalInfo/gender",
			"type": "SINGLE_SELECTION_RADIO"
		},
		{
			"name": "birthdate",
			"xpath": "/PersonalInfo/birthday",
			"type": "INPUT",
			"format": "DATE",
			"subformat": "DATE_PAST"
		},
		{
			"name": "address",
			"xpath": "/PersonalInfo/street",
			"type": "INPUT",
			"format": "TEXT",
			"subformat": "TEXT"
		},
		{
			"name": "postalcode",
			"xpath": "/PersonalInfo/PostalCode",
			"type": "INPUT",
			"format": "POSTAL_CODE",
			"subformat": "POSTAL_CODE"
		},
		{
			"name": "city",
			"xpath": "/PersonalInfo/city",
			"type": "INPUT",
			"format": "TEXT",
			"subformat": "TEXT"
		},
		{
			"name": "mobilePhone",
			"xpath": "/PersonalInfo/phone",
			"type": "INPUT",
			"format": "TEXT",
			"subformat": "PHONE"
		},
		{
			"name": "email",
			"xpath": "/PersonalInfo/email",
			"type": "INPUT",
			"format": "TEXT",
			"subformat": "EMAIL"
		},
		{
			"name": "comparationId",
			"xpath": "/PersonalInfo/comparationId",
			"type": "SYSTEM_FIELD",
			"format": "TEXT",
			"subformat": "TEXT"
		}
	]
}