<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite thread-count="1" verbose="1" name="WebFormsCore">
	<test name="WebFormsTest" enabled="true" preserve-order="true">
		<groups>
			<run>
				<include name="login" />
				<include name="basicFunctionality" />
				<include name="formBlockDesign" />
				<include name="formFlow" />
				<include name="export" />
				<include name="impactAnalysis" />
				<include name="xForms" />
				<include name="validation" />
			</run>
		</groups>
		<classes>
			<class name="com.biit.webforms.gui.test.LoginTests" />
			<class name="com.biit.webforms.gui.test.BasicFuncionalityTests" />
			<class name="com.biit.webforms.gui.test.FormBlockDesignTests" />
			<class name="com.biit.webforms.gui.test.FormFlowTests" />
			<class name="com.biit.webforms.gui.test.ExportTests" />
			<class name="com.biit.webforms.gui.test.ImpactAnalysisTests" />
			<class name="com.biit.webforms.gui.test.XFormsTests" />
			<class name="com.biit.webforms.gui.test.ValidationTests" />
		</classes>
	</test>
</suite>